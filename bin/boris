#!/usr/bin/env php
<?php

declare (strict_types=1);

require_once __DIR__.'/../bootstrap.php';
require_once 'phar://' . __DIR__ . '/../boris.phar/lib/autoload.php';

set_time_limit(0);
restore_error_handler();
restore_exception_handler();

(new Boris\Boris(
    sprintf('%s-%s> ', APP_NAME, APP_VERSION)
))->start();
