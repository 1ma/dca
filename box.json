{
  "alias": "dca.phar",
  "chmod": "0755",
  "directories": ["bin", "src"],
  "compactors": [
    "Herrera\\Box\\Compactor\\Php"
  ],
  "extract": true,
  "files": [
    "bootstrap.php",
    "LICENSE",
    "settings.ini"
  ],
  "finder": [
    {
      "name": ["*.php", "*.pem"],
      "exclude": ["tests"],
      "in": ["vendor"]
    }
  ],
  "git-version": "package_version",
  "main": "bin/console",
  "output": "dca.phar",
  "stub": true
}
